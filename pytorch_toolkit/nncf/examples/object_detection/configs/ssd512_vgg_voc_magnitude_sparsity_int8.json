{
  "model": "ssd_vgg",
  "batchnorm": true,
  "input_info": {
    "sample_size": [8, 3, 512, 512]
  },
  "pretrained": true,
  "num_classes": 21,
  "dataset": "voc",
  "preprocessing": {
    "mean": [0.406, 0.456, 0.485],
    "std": [0.255, 0.224, 0.229],
    "normalize_coef": 255,
    "rgb": true
  },
  "max_iter": 30000,
  "batch_size": 96,
  "iter_size": 1,
  "save_freq": 3, // in epochs
  "epochs": 111,
  "optimizer": {
    "type": "Adam",
    "base_lr": 1e-4,
    "weight_decay": 5e-4,
    "schedule_type": "multistep",
    "steps": [1, 1]
  },
  "ssd_params": {
    "steps": [8, 16, 32, 64, 128, 256, 512],
    "min_sizes": [35.84, 76.8, 153.6, 230.4, 307.2, 384.0, 460.8],
    "max_sizes": [76.8, 153.6, 230.4, 307.2, 384.0, 460.8, 537.6],
    "aspect_ratios": [[2], [2, 3], [2, 3], [2, 3], [2, 3], [2], [2]],
    "variance": [0.1, 0.1, 0.2, 0.2],
    "clip": false,
    "flip": true
  },
  "compression": [
    {
      "algorithm": "magnitude_sparsity",
      "params": {
        "schedule": "multistep",
        "multistep_sparsity_levels": [0.3, 0.5, 0.7],
        "multistep_steps": [40, 80]
      }
    },
    {
      "algorithm": "quantization",
      "initializer": {
        "range": {
          "num_init_steps": 10
        }
      }
    }
  ]
}
