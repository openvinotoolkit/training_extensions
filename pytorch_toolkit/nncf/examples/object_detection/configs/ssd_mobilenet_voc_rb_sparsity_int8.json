{
  "model": "ssd_mobilenet",
  "batchnorm": true,
  "input_info": {
    "sample_size": [8, 3, 300, 300]
  },
  "num_classes": 21,
  "dataset": "voc",
  "preprocessing": {
    "mean": [0.406, 0.456, 0.485],
    "std": [0.255, 0.224, 0.229],
    "normalize_coef": 255,
    "rgb": true
  },
  "max_iter": 100000,
  "batch_size": 256,
  "iter_size": 1,
  "save_freq": 50,
  "optimizer": {
    "type": "Adam",
    "base_lr": 1e-4,
    "weight_decay": 5e-4,
    "schedule_type": "multistep",
    "steps": [300, 320],
    "parameter_groups": [
      {"lr":  1e-3, "weight_decay":  0, "re": ".*\\._mask"},
      {"lr":  1e-4, "weight_decay":  1e-4}
    ]
  },
  "ssd_params": {
    "variance": [0.1, 0.1, 0.2, 0.2],
    "max_sizes": [60, 111, 162, 213, 264, 315],
    "min_sizes": [30, 60, 111, 162, 213, 264],
    "steps": [16, 32, 64, 100, 150, 300],
    "aspect_ratios": [[2], [2, 3], [2, 3], [2, 3], [2], [2]],
    "clip": false,
    "flip": true,
    "top_k": 20
  },
  "compression": [
    {
      "algorithm": "rb_sparsity",
      "params": {
        "schedule": "adaptive",
        "sparsity_init": 0.3,
        "sparsity_target": 0.9,
        "sparsity_steps": 75000,
        "sparsity_training_steps": 75000
      }
    },
    {
      "algorithm": "quantization",
      "initializer": {
        "num_init_steps": 1
      }
    }
  ]
}

