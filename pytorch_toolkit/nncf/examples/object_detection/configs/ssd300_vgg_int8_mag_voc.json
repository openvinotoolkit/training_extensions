{
  "model": "ssd_vgg",
  "weights": "<WEIGHTS_PATH>",
  "batchnorm": true,
  "input_sample_size": [8, 3, 300, 300],
  "num_classes": 21,
  "dataset": "voc",
  "preprocessing": {
    "mean": [0.406, 0.456, 0.485],
    "std": [0.255, 0.224, 0.229],
    "normalize_coef": 255,
    "rgb": true
  },
  "max_iter": 30000,
  "batch_size": 128,
  "iter_size": 1,
  "save_freq": 10,
  "optimizer": {
    "type": "Adam",
    "base_lr": 1e-4,
    "weight_decay": 1e-4,
    "schedule_type": "multistep",
    "steps": [100, 110]
  },
  "ssd_params": {
    "clip": false,
    "variance": [0.1, 0.1, 0.2, 0.2],
    "max_sizes": [60, 111, 162, 213, 264, 315],
    "min_sizes": [30, 60, 111, 162, 213, 264],
    "steps": [8, 16, 32, 64, 100, 300],
    "aspect_ratios": [[2], [2, 3], [2, 3], [2, 3], [2], [2]],
    "flip": true,
    "top_k": 200
  },
  "compression": [
    {
      "algorithm": "magnitude_sparsity",
      "params": {
        "schedule": "multistep",
        "sparsity_levels": [0.3, 0.5, 0.7],
        "steps": [40, 80]
      }
    },
    {
      "algorithm": "quantization",
      "initializer": {
        "num_init_steps": 10
      }
    }
  ]
}