{
  "model": "ssd_mobilenet",
  "batchnorm": true,
  "input_info": {
    "sample_size": [1, 3, 300, 300]
  },
  "pretrained": true,
  "num_classes": 21,
  "dataset": "voc",
  "preprocessing": {
    "mean": [0.406, 0.456, 0.485],
    "std": [0.255, 0.224, 0.229],
    "normalize_coef": 255,
    "rgb": true
  },
  "max_iter": 75000,
  "iter_size": 1,
  "save_freq": 50,
  "optimizer": {
    "type": "Adam",
    "base_lr": 1e-4,
    "weight_decay": 5e-4,
    "schedule_type": "plateau",
    "scheduler_params": {
      "threshold": 0.1,
      "cooldown": 3
    }
  },
  "ssd_params": {
    "variance": [0.1, 0.1, 0.2, 0.2],
    "max_sizes": [60, 111, 162, 213, 264, 315],
    "min_sizes": [30, 60, 111, 162, 213, 264],
    "steps": [16, 32, 64, 100, 150, 300],
    "aspect_ratios": [[2], [2, 3], [2, 3], [2, 3], [2], [2]],
    "clip": false,
    "flip": true,
    "top_k": 200,
  }
}

