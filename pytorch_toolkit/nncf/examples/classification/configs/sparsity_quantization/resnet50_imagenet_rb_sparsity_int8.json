{
    "model": "resnet50",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 256,
    "epochs": 23,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.001,
        "schedule_type": "multistep",
        "steps": [15, 20, 25]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "params": {
                "sparsity_init": 0.01,
                "sparsity_target": 0.61,
                "sparsity_steps": 5,
                "sparsity_training_steps": 10
            }
        },
        {
            "algorithm": "quantization",
            "initializer": {
                "num_init_steps": 1
            }
        }
    ]
}
