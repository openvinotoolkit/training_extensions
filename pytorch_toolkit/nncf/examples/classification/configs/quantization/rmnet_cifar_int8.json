{
    "model": "RMNetClassifierCifar",
    "dataset": "CIFAR100",
    "num_classes": 100,
    "pretrained": true,
    "batch_size": 256,
    "epochs": 300,
    "input_info": {
      "sample_size": [2, 3, 32, 32]
    },
    "optimizer": {
        "type": "Adam",
        "base_lr": 0.0001,
        "schedule_type": "multistep",
        "steps": [
            20,
            45
        ]
    },
    "compression": {
        "algorithm": "quantization",
        "scope_overrides": {
            "{re}RMBlock\\[[0-9]*\\]/ELU\\[activation\\]": {
                "signed": true
            }
        }
    }
}
