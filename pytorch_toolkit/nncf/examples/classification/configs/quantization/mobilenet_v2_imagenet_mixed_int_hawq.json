{
    "model": "mobilenet_v2",
    "pretrained": true,
    "input_info": {
        "sample_size": [
            2,
            3,
            224,
            224
        ]
    },
    "num_classes": 1000,
    "batch_size": 200,
    "workers": 6,
    "epochs": 100,
    "multiprocessing_distributed": true,
    "optimizer": {
        "base_lr": 3.1e-4,
        "schedule_type": "plateau",
        "type": "Adam",
        "scheduler_params": {
            "threshold": 0.1,
            "cooldown": 3
        },
        "weight_decay": 1e-05
    },
    "compression": {
        "algorithm": "quantization",
        "weights": {
            "mode": "asymmetric",
            "per_channel": true,
            "bits": 4
        },
        "activations": {
            "mode": "asymmetric",
            "bits": 8
        },
        "quantize_inputs": true,
        "initializer": {
            "precision": {
                "type": "hawq",
                "bits": [
                    4,
                    8
                ],
                "num_data_points": 100,
                "iter_number": 200,
                "tolerance": 1e-4
            }
        }
    }
}
