{
    "model": "icnet",
    "dataset" : "cityscapes",

    "preprocessing": {
        "resize": {
            "height": 1024,
            "width": 2048
        },
        "normalize":
        {
            "mean": [0.290101, 0.328081, 0.286964],
            "std": [0.182954, 0.186566, 0.184475]
        }
    },

    "augmentations": {
        "random_hflip": 0.5,
        "random_sized_crop": 0.5,
        "resize": {
            "height": 1024,
            "width": 2048
        }
    },

    "input_info": {
      "sample_size": [1, 3, 1024, 2048]
    },

    "weighing": [0.0000, 0.0329, 0.1994, 0.0532, 1.4976, 1.2618, 0.9889, 5.5597, 2.2015,
        0.0763, 1.0062, 0.3019, 0.9938, 7.2385, 0.1736, 1.7641, 1.6313, 0.9323,
        6.4544, 2.8066],

    "optimizer": {
        "type": "adam",
        "optimizer_params": {
            "lr": 1.0e-3,
            "weight_decay": 2.0e-4
        },
        "schedule_type": "poly",
        "scheduler_params": {
            "power": 0.9
        }
    },

    "model_params": {
         "in_channels": 3,
         "input_size_hw": [1024, 2048]
    }
}