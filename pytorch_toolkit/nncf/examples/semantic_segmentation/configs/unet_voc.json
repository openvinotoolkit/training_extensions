{
    "model": "unet",
    "dataset" : "voc",
    "weighing": "mfb",

    "augmentations":
    {
        "random_hflip": 0.5
    },

    "preprocessing": {
        "resize": {
            "height": 512,
            "width": 512
        },
        "normalize": {
            "mean": [0.485, 0.456, 0.406],
            "std" : [0.229, 0.224, 0.225]
        }
    },

    "input_info": {
      "sample_size": [1, 3, 512, 512]
    },

    "optimizer": {
        "type": "Adam",
        "optimizer_params": {
            "lr": 5.0e-4,
            "weight_decay": 2.0e-4
        },
        "schedule_type": "step",
        "step": 100,
        "gamma": 0.1
    },

    "model_params": {
         "in_channels": 3,
         "batch_norm": true
    }
}