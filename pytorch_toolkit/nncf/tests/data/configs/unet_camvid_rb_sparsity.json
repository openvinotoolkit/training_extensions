{
    "model": "unet",
    "dataset" : "camvid",

    "preprocessing": {
        "resize": {
            "height": 368,
            "width": 480
        },
        "normalize":
        {
            "mean": [0.39068785, 0.40521392, 0.41434407],
            "std" : [0.29652068, 0.30514979, 0.30080369]
        }
    },


    "input_info": {
      "sample_size": [1, 3, 368, 480]
    },

    "weighing": "mfb",

    "optimizer": {
        "type": "Adam",
        "optimizer_params": {
            "lr": 5.0e-5,
            "weight_decay": 2.0e-4
        },
        "schedule_type": "step",
        "step": 100,
        "gamma": 0.1
    },

    "model_params": {
         "input_size_hw": [368, 480]
    },
    "compression": {
        "algorithm": "rb_sparsity"
    }
}