{
    "model": "squeezenet1_1_custom",
    "pretrained": false,
    "input_info": {
      "sample_size":  [1, 3, 32, 32]
    },
    "num_classes": 10,
    "optimizer": {
        "type": "sgd",
        "base_lr": 1e-2,
        "schedule_type": "multistep",
        "steps": [
            1
        ]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "params": {
                "schedule": "polynomial",
                "sparsity_init": 0.2,
                "sparsity_target": 0.6,
                "sparsity_steps": 5
            }
        },
        {
            "algorithm": "quantization",
            "weights": {
                "per_channel": true
            }
        }
    ]
}