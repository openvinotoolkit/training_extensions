{
    "model": "resnet50",
    "pretrained": true,

    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size": 85,
    "epochs": 1,
    "multiprocessing_distributed": true,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-5
    },
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "num_init_steps": 10
        }
    }
}
